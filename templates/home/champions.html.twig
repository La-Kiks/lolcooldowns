{% extends 'base.html.twig' %}

{% block title %}LoL Cooldowns{% endblock %}

{% block body %}
    <h1> Champion Page Test</h1>
    {% if form is defined %}
        {% include 'components/_search_data.html.twig' with {
            form: form
        } only %}
    {% endif %}
<div class="container text-center">
    {% for champion in champions %}
        {% include 'components/_champion_view.html.twig' %}
    {% endfor %}
    <div class="container">
        {{  knp_pagination_render(champions) }}
    </div>
</div>
{% endblock %}